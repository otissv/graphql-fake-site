{
  "name": "squadron-authentication",
  "version": "1.0.0",
  "description": "GraphQL Authentication Service with Node and Redis ",
  "scripts": {
    "start": "node ./build/server/index.js",
    "dev": "nodemon --exec yarn run babel-node  -- ./src/server/index.js",
    "build:clean": "rm -rf build && mkdir build",
    "build:server": "babel -d ./build ./src -s --ignore node_modules,public",
    "build:server:pro": "babel -d ./build ./src --ignore node_modules,public",
    "build": "npm run build:clean && npm run build:server",
    "build:pro": "npm run build:clean && npm run build:server:pro",
    "build:install": "yarn install --ignore-engines --production",
    "babel-node": "babel-node",
    "lint": "eslint --quiet './' && echo 'Completed:' && date || true && echo 'Completed:'",
    "lint:watch": "watch 'npm run lint' ./",
    "tape": "babel-node test-runner.js | tap-min || true",
    "tape:spec": "babel-node test-runner.js | tap-difflet || true",
    "test": "npm run tape",
    "test:spec": "npm run tape:spec",
    "test:watch": "watch 'npm run tape' ",
    "seed": "npm run babel-node -- ./src/seed/index.js",
    "seed:pro": "node ./build/seed/seed.js"
  },
  "author": "Otis Virginie",
  "keywords": [
    "node",
    "express",
    "api",
    "graphql",
    "jwt",
    "json web token",
    "redis",
    "es6"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "~0.15.3",
    "babel-cli": "~6.24.0",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "~3.5.0",
    "body-parser": "~1.17.1",
    "casual": "^1.5.11",
    "compression": "~1.6.2",
    "connect-mongo": "~1.3.2",
    "cookie-parser": "~1.4.3",
    "cors": "~2.8.1",
    "csurf": "~1.9.0",
    "express": "~4.15.2",
    "express-enforces-ssl": "~1.1.0",
    "express-handlebars": "^3.0.0",
    "express-logger": "0.0.3",
    "express-session": "~1.15.1",
    "faker": "^4.1.0",
    "get-env": "^0.5.10",
    "graphql": "^0.9.1",
    "graphql-server-express": "^0.6.0",
    "graphql-tools": "^0.10.1",
    "helmet": "~3.5.0",
    "is-schema-valid": "~0.2.0",
    "isemail": "~2.2.1",
    "jsonwebtoken": "~7.3.0",
    "libphonenumber-node": "0.0.3",
    "map-async-promise": "~2.0.1",
    "method-override": "~2.3.7",
    "morgan": "~1.8.1",
    "nodemon": "~1.11.0",
    "promise-redis": "0.0.5",
    "ramda": "~0.23.0",
    "redis": "~2.7.1",
    "rethinkdbdash": "^2.3.28",
    "squadron-utils": "^0.0.1",
    "ufunc": "~2.0.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "~7.1.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-2": "~6.22.0",
    "eslint": "~3.17.1",
    "eslint-config-standard": "~7.0.1",
    "eslint-plugin-graphql": "~0.7.0",
    "eslint-plugin-promise": "~3.5.0",
    "eslint-plugin-standard": "~2.1.1",
    "faucet": "0.0.1",
    "glob": "~7.1.1",
    "tap-difflet": "~0.7.0",
    "tap-min": "~1.2.1",
    "tape": "~4.6.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
